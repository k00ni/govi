{
    "type": "project",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "sweetrdf/easyrdf": "^1.11.0",
        "twig/twig": "^3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2.0",
        "phpstan/phpstan": "^1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
